{
  "name": "ipa-ai-agents",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "concurrently \"npm:tailwind:watch\" \"npm:start\"",
    "tailwind:build": "npx postcss src/styles.css -o src/generated-tailwind.css",
    "tailwind:watch": "npx postcss src/styles.css -o src/generated-tailwind.css --watch",
    "prestart": "npm run prebuild",
    "prebuild": "npx postcss src/styles.css -o src/generated-tailwind.css",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.0.3",
    "@angular/cdk": "^21.0.1",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.1",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@tailwindcss/postcss": "^4.1.17",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.0",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "jsdom": "^27.1.0",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.57.0",
    "pngjs": "^7.0.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
